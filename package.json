  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "mongoose": "^5.3.0",
    "popper.js": "^1.14.4",
    "react-spinners": "^0.4.5",
    "reactstrap": "^6.4.0"
  }
}
